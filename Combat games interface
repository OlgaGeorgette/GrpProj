    private void jButton1MouseClicked(java.awt.event.MouseEvent evt) {                                      
           jButton1.setVisible(true);
           this.dispose();
           
           //back to the dashboard
           GenreDashboard board = new GenreDashboard();
           board.setVisible(true);
                  
    }                                     

